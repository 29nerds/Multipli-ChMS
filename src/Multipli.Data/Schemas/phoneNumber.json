{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "PhoneNumber",
  "type": "object",
  "description": "A schema for a general phone number.",
  "properties": {
    "id": {
      "type": "object",
      "description": "A unique identifier."
    },
    "number": {
      "type": "string",
      "description": "The telephone number."
    },
    "phoneNumberType": {
      "enum": [ "Mobile"]
    },
    "organizationId": { "$ref": "http://multipli.io/organization#" },
    "localtionId": { "$ref": "http://multipli.io/location#" }
  },
  "required": [
    "id",
    "number",
    "phoneNumberType"
  ]
}
